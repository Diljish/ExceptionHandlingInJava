package com.exceptionhandling;

import java.util.Scanner;

public class FinallyRunner {

	public static void main(String[] args) {

		Scanner scanner = new Scanner(System.in);
		
		try {
			int[] numbers = {1,2,3,4};
			int number = numbers[5];
			
		}catch(Exception e ) {
			
		}finally {
			scanner.close();
		}
		
		
		
		
	}

}
