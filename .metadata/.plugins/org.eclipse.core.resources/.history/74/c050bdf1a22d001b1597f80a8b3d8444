package com.exceptionhandling;

public class ExceptionHandlingRunner_2 {

	public static void main(String[] args) {
		
		methode1();
	}
	public static void methode1() {
		methode2();
		
	}
	public static void methode2() {
		try {
			String name = null;
			name.length();
		}catch(Exception e) {
			e.printStackTrace();
		}
		
		
	}
}
